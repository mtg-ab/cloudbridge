  "scripts": {
    "clean:dist": "rm -rf dist && mkdir dist ",
    "build": "yarn clean:dist && yarn build:gulp && yarn build:webpack",
    "build:webpack": "webpack",
    "build:gulp": "gulp build",
    "watch": "gulp watch",
    "watch:webpack": "webpack --watch --progress",
    "gen:fields": "gulp generate-fields",
    "gen:module": "yarn plop; yarn gen:fields",
    "hs:auth": "hs auth personalaccesskey",
    "hs:sync-prod": "yarn gen:fields; hs upload --overwrite --portal=prod  dist MTG-HS-Boilerplate",
    "hs:sync-dev": "yarn gen:fields;  hs upload --overwrite --portal=mtg-lab-1  dist MTG-HS-Boilerplate-V2",
    "hs:module": "yarn plop; yarn gen:fields",
    "hs:watch": "hs watch --portal=mtg-lab-1 dist MTG-HS-Boilerplate-V2",
    "deploy:prod": "yarn build && yarn hs:sync-prod",
    "deploy:dev": "yarn build && yarn hs:sync-dev"
  },
